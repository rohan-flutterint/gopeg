Text: (#Sequence* EndOfLine)*
EndOfLine: ";"
#Sequence: (
    =~"[\t\r\n ]+" /
    =~"//[^\n]+" /
    "/*" (!"*/" .)* "*/" /
    String:(=~"'(\\.|[^'\\\\])*'" / =~"\"(\\.|[^\\\"\\\\])*\"") /
    Number:=~"[0-9_]+(\\.[0-9]+)?" /
    Token:=~"[#a-zA-Z][0-9a-zA-Z_]*" /
    Control:(=~"[!+/*,-.%]" / "<=" / ">=" / "==" / "=" / ("<" !"=") / (">" !"=")) /
    Open:"(" #Sequence* Close:")"
)
